{{! The main content area on the homepage }}
<main id="content" name="content" class="content wrapper-work" style="padding-top:0px!important" role="main">
  <div class="container-fluid" style="padding:0px">
    <div class="row  wrapper-desc" style="margin-bottom:0rem">
      <hr class="dash">
      <h2 style="color:#323852" class=" section-titles no-padding">WORK.</h2>{{!-- TABS --}}
      <div class="row" style="width:100%;margin:auto;align-items:flex-start;">
        <div class="desktop-menu" style="padding-bottom:1rem; justify-content:flex-start">
          <a class="tab-text tab-text-fx" onclick="showTab(event, 'ALLtab')" id="defaultOpen" data-letters="ALL">ALL</a>
          <a class="tab-text tab-text-fx" onclick="showTab(event, 'UItab')" data-letters="UI">UI</a>
          <a class="tab-text tab-text-fx" onclick="showTab(event, 'UXtab')" data-letters="UX">UX </a>
          <a class="tab-text tab-text-fx" onclick="showTab(event, 'Devtab')" data-letters="DEV">DEV </a>
        </div>
      </div>
      {{!-- END --}}
    </div>
    {{> "loop"}}
  </div>
</main>

